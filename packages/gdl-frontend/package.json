{
  "name": "gdl-frontend",
  "private": true,
  "version": "0.0.1",
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "srcPathDirs": [
      "<rootDir>/components/",
      "<rootDir>/pages/",
      "<rootDir>/hocs/",
      "<rootDir>/elements/"
    ],
    "format": "minimal"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "browser": {
    "dns": false
  },
  "scripts": {
    "dev": "node server",
    "build": "npm run i18n:extract && npm run i18n:compile && next build",
    "start": "NODE_ENV=production node server",
    "i18n:extract": "node_modules/@lingui/cli/lingui.js extract --clean",
    "i18n:compile": "node_modules/@lingui/cli/lingui.js compile",
    "i18n:add-locale": "node_modules/@lingui/cli/lingui.js add-locale",
    "analyze": "cross-env ANALYZE=1 next build"
  },
  "dependencies": {
    "@engineerapart/nextscript": "1.0.2",
    "@lingui/cli": "2.7.1",
    "@lingui/react": "2.7.1",
    "@lingui/babel-preset-react": "2.7.1",
    "@material-ui/core": "3.6.1",
    "@material-ui/icons": "3.0.1",
    "@sentry/browser": "4.4.2",
    "@sentry/node": "4.4.2",
    "@zeit/next-source-maps": "0.0.3",
    "auth0-js": "9.8.1",
    "babel-plugin-emotion": "9.2.11",
    "clean-webpack-plugin": "1.0.0",
    "cookie-parser": "^1.4.3",
    "copy-to-clipboard": "3.0.8",
    "emotion": "9.2.12",
    "emotion-server": "9.2.12",
    "express": "4.16.4",
    "express-request-language": "^1.1.15",
    "facepaint": "1.2.1",
    "gdl-auth": "^0.0.0",
    "gdl-config": "^0.0.0",
    "gdl-image": "^0.0.0",
    "glob": "^7.1.2",
    "helmet": "3.14.0",
    "invariant": "^2.2.4",
    "isomorphic-unfetch": "3.0.0",
    "jwt-decode": "^2.2.0",
    "localforage": "1.7.3",
    "lru-cache": "^4.1.3",
    "lscache": "1.2.0",
    "next": "7.0.2",
    "next-plugin-transpile-modules": "0.1.3",
    "next-routes": "1.4.2",
    "polished": "2.3.0",
    "react": "16.6.3",
    "react-content-loader": "3.4.1",
    "react-dom": "16.6.3",
    "react-emotion": "9.2.12",
    "react-ga": "2.5.3",
    "react-joyride": "2.0.5",
    "react-jss": "8.6.1",
    "react-swipeable": "^4.3.0",
    "universal-cookie": "3.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.4",
    "cross-env": "^5.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "^3.3.4",
    "jest-emotion": "9.2.11",
    "react-test-renderer": "16.6.3",
    "service-worker-mock": "1.10.1",
    "webpack-bundle-analyzer": "3.0.3"
  }
}
