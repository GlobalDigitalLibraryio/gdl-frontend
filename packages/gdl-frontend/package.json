{
  "name": "gdl-frontend",
  "private": true,
  "version": "0.0.1",
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "srcPathDirs": [
      "<rootDir>/components/",
      "<rootDir>/pages/",
      "<rootDir>/hocs/",
      "<rootDir>/elements/"
    ]
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "browser": {
    "dns": false,
    "raven": "raven-js"
  },
  "scripts": {
    "dev": "node server",
    "build": "npm run i18n:extract && npm run i18n:compile && next build",
    "start": "NODE_ENV=production node server",
    "i18n:extract": "node_modules/@lingui/cli/lingui.js extract --clean",
    "i18n:compile": "node_modules/@lingui/cli/lingui.js compile",
    "i18n:add-locale": "node_modules/@lingui/cli/lingui.js add-locale",
    "analyze": "cross-env ANALYZE=1 next build"
  },
  "dependencies": {
    "@lingui/react": "2.0.8",
    "@material-ui/core": "1.3.1",
    "@material-ui/icons": "^1.1.0",
    "@zeit/next-source-maps": "^0.0.2",
    "auth0-js": "9.6.1",
    "cookie-parser": "^1.4.3",
    "emotion": "9.2.5",
    "emotion-server": "9.2.5",
    "emotion-theming": "9.2.5",
    "express": "^4.16.3",
    "express-request-language": "^1.1.15",
    "glob": "^7.1.2",
    "helmet": "^3.12.1",
    "invariant": "^2.2.4",
    "isomorphic-unfetch": "^2.0.0",
    "jwt-decode": "^2.2.0",
    "lru-cache": "^4.1.3",
    "lscache": "^1.1.0",
    "next": "6.1.1",
    "next-routes": "^1.4.2",
    "polished": "^1.9.2",
    "prop-types": "15.6.2",
    "raven": "2.6.3",
    "raven-js": "3.26.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "9.2.5",
    "react-ga": "^2.5.2",
    "react-swipeable": "^4.3.0",
    "styled-system": "^2.2.5",
    "universal-cookie": "^2.1.5"
  },
  "devDependencies": {
    "@lingui/babel-preset-react": "^2.0.8",
    "@lingui/cli": "^2.0.8",
    "babel-plugin-emotion": "9.2.5",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-transform-assets": "^0.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest-emotion": "9.2.5",
    "nock": "^9.2.6",
    "react-test-renderer": "^16.4.1",
    "cross-env": "^5.0.1",
    "webpack-bundle-analyzer": "^2.8.2"
  }
}
