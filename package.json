{
  "name": "gdl-frontend",
  "version": "0.0.1",
  "description": "GDL Frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/GlobalDigitalLibraryio/gdl-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/GlobalDigitalLibraryio/gdl-frontend/issues"
  },
  "engines": {
    "node": ">=6.6.0",
    "npm": "~3.10.3"
  },
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "srcPathIgnorePatterns": [
      "/node_modules/",
      "/.next"
    ]
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "author": "gdl@knowit.no",
  "license": "GPL-3.0",
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "eslint --ext .js --ext .jsx pages components hocs",
    "dev": "npm run i18n:compile && node server.js",
    "build": "npm run i18n:compile && next build",
    "start": "NODE_ENV=production node server.js",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "i18n:add-locale": "lingui add-locale",
    "format": "prettier-eslint --write \"components/**/*.js\" \"pages/**/*.js\" \"hocs/**/*.js\" \"style/**/*.js\"",
    "flow": "flow",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "domurl": "2.1.7",
    "downshift": "1.18.0",
    "express": "4.16.2",
    "express-request-language": "1.1.12",
    "glob": "7.1.2",
    "isomorphic-unfetch": "2.0.0",
    "lingui-react": "1.4.1",
    "next": "4.1.4",
    "next-routes": "1.1.0",
    "polished": "1.9.0",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-icons": "2.2.7",
    "react-swipeable": "4.2.0",
    "serialize-javascript": "1.4.0",
    "styled-components": "2.2.3",
    "styled-system": "1.0.8"
  },
  "devDependencies": {
    "@storybook/react": "3.2.16",
    "babel-eslint": "7.2.3",
    "babel-plugin-inline-react-svg": "0.5.2",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-styled-components": "1.3.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-lingui-react": "1.0.11",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-to-json": "3.2.2",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.8.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "0.56.0",
    "jest": "21.3.0-beta.2",
    "jest-styled-components": "4.9.0",
    "lingui-cli": "1.4.0",
    "nock": "^9.1.0",
    "prettier-eslint": "8.2.2",
    "prettier-eslint-cli": "4.4.0",
    "react-test-renderer": "16.1.1"
  }
}
