{
  "name": "gdl-frontend",
  "version": "0.0.1",
  "description": "GDL Frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/GlobalDigitalLibraryio/gdl-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/GlobalDigitalLibraryio/gdl-frontend/issues"
  },
  "engines": {
    "node": ">=6.6.0",
    "npm": "~3.10.3"
  },
  "lingui": {
    "fallbackLocale": "en",
    "srcPathIgnorePatterns": [
      "/node_modules/",
      "/.next"
    ]
  },
  "author": "gdl@knowit.no",
  "license": "GPL-3.0",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint --ext .js --ext .jsx pages components",
    "dev": "npm run i18n:compile && node server.js",
    "build": "npm run i18n:compile && next build",
    "start": "NODE_ENV=production node server.js",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "i18n:add-locale": "lingui add-locale",
    "format": "prettier-eslint --write \"components/**/*.js\" \"pages/**/*.js\"",
    "flow": "flow"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "domurl": "^2.1.7",
    "express": "^4.15.4",
    "express-request-language": "^1.1.12",
    "glob": "^7.1.2",
    "isomorphic-unfetch": "^2.0.0",
    "lingui-react": "^1.2.0",
    "next": "^3.2.1",
    "next-routes": "^1.0.40",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scroll": "^1.5.4",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-lingui-react": "^1.0.5",
    "eslint": "^4.5.0",
    "eslint-config-ndla": "^0.3.3",
    "flow-bin": "^0.53.1",
    "lingui-cli": "^1.3.2",
    "prettier-eslint": "^6.4.2",
    "prettier-eslint-cli": "^4.2.1"
  }
}
