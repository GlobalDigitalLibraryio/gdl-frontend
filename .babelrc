{
  "env": {
    "development": {
      "presets": ["next/babel", "lingui-react"]
    },
    "production": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": { "targets": { "ie": 11 } }
          }
        ],
        "lingui-react"
      ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel",
        "lingui-react"
      ]
    }
  },
  "plugins": [
    [
      "transform-assets",
      {
        "extensions": ["png", "jpg"],
        "regExp": ".*/static/(.+)",
        "name": "/static/[1]?[sha512:hash:base64:7]"
      }
    ],
    "polished",
    "transform-flow-strip-types",
    "inline-react-svg",
    "emotion"
  ]
}
